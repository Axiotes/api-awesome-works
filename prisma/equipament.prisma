model Equipament {
  id        Int               @id @default(autoincrement())
  name      String            @unique
  prefix    String            @db.VarChar(3)
  category  String            @db.VarChar(50)
  brand     String            @db.VarChar(50)
  active    Boolean           @default(true)
  createdAt DateTime          @default(now())
  updatedAt DateTime?         
  deletedAt DateTime?

  items     EquipamentItem[]
}